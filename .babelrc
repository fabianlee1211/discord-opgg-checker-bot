{
  "presets": [
    [
      "@babel/preset-env",
      {
        "targets": {
          "node": "current"
        }
      }
    ],
    "@babel/preset-typescript"
  ],
  "plugins": [
    [
      "module-resolver",
      {
        "alias": {
          "@models": "./src/models",
          "@storage": "./src/storage",
          "@typings": "./src/typings"
        },
        "root": ["./src"],
        "extensions": [".js", ".ts"]
      }
    ]
  ]
}
